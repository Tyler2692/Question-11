#include <iostream>
using namespace std;

int main()
{
    double tanksize = 20; //number of gallons in tank
    double mpgtown = 23.5; // miles per gallon used in town
    double mpghighway = 28.9; // miles per gallon used on highway

    double distancetown = tanksize * mpgtown;
    double distancehighway = tanksize * mpghighway;

    cout << "distance driven in town is " << distancetown << " miles and distance driven on highway is " << distancehighway << " miles" << endl;
    return 0;
}
